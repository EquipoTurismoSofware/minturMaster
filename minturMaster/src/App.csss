  -webkit-border-radius: 2px;
  border-radius: 2px;
  background-color: #ed0f68;
  color: white;
  cursor: pointer;
  transition: all 1.5s;
}
.ViewerListados .pages ul li.selected {
  background-color: #722789;
  color: white;
}
.ViewerListados .btn-left,
.ViewerListados .btn-right,
.ViewerListados .pages {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: center;
  align-items: center;
}
.ViewerListados .btn-left,
.ViewerListados .btn-right {
